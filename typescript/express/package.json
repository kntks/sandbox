{
  "name": "express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run start:nodemon",
    "start:node-dev": "ts-node-dev -r tsconfig-paths/register --poll --exit-child --watch src src/index.ts",
    "start:nodemon": "nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "dev": "nodemon dist/app.js",
    "test": "jest --coverage --passWithNoTests --runInBand",
    "build:swc": "swc src -d dist && tsc-alias -p tsconfig.json",
    "build:tsc": "tsc --noEmitOnError -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build": "npm run build:prisma && npm run build:swc",
    "build:prisma": "prisma migrate deploy",
    "lint:fix": "eslint --fix './src/**/*.{js,ts}' && prettier --write './src/**/*.{js,ts}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.196",
    "@swc/jest": "^0.2.21",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.0",
    "@types/node": "^17.0.29",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.8",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "express": "^4.18.0",
    "express-validator": "^6.14.0"
  }
}
